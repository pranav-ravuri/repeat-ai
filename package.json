{
	"name": "sveltekit-starter-prismic-minimal",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "concurrently \"npm:vite:dev\" \"npm:slicemachine\" --prefix-colors blue,magenta",
		"vite:dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"slicemachine": "start-slicemachine"
	},
	"dependencies": {
		"@fontsource-variable/dm-sans": "^5.2.8",
		"@prismicio/client": "^7.20.1",
		"@prismicio/svelte": "^2.0.0",
		"@tailwindcss/vite": "^4.1.13",
		"clsx": "^2.1.1",
		"gsap": "^3.13.0",
		"tailwindcss": "^4.1.13"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.9",
		"@eslint/js": "^9.28.0",
		"@slicemachine/adapter-sveltekit": "^0.3.84",
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/kit": "^2.43.5",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@tailwindcss/typography": "^0.5.19",
		"@types/eslint": "^9.6.1",
		"concurrently": "^9.1.2",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.9.1",
		"globals": "^16.2.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"slice-machine-ui": "^2.18.2",
		"svelte": "^5.33.14",
		"svelte-check": "^4.2.1",
		"tslib": "^2.8.1",
		"typescript": "^5.3.3",
		"typescript-eslint": "^8.33.1",
		"vite": "^6.3.6",
		"vite-plugin-devtools-json": "^1.0.0"
	},
	"overrides": {
		"cookie": ">=0.7.0"
	}
}
